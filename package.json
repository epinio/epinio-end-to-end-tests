    "cypress-dark": "^1.8.3",
    "cypress-file-upload": "^5.0.8",
    "dotenv": "^10.0.0",
    "typescript": "^4.5.2"
  },
  "devDependencies": {
    "cypress-multi-reporters": "^1.6.1",
